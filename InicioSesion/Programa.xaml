<Window x:Class="InicioSesion.Programa"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InicioSesion"
        mc:Ignorable="d"
        Title="Programa" MinHeight="450" MinWidth="800" MaxHeight="449" MaxWidth="799">
    <Grid Background="#7FE0760B">
        <StackPanel x:Name="panelJefe" Margin="53,39,68,75">
            <Label Content="Jefe" FontWeight="Bold" FontSize="20" FontFamily="Segoe UI Black"/>
        </StackPanel>
        <StackPanel x:Name="panelAdministrador" Margin="53,39,68,75">
            <Label Content="Administrador" FontWeight="Bold" FontSize="20" FontFamily="Segoe UI Black"/>
        </StackPanel>
        <StackPanel x:Name="panelTrabajador" Margin="52,39,68,105">
            <Label Content="Trabajador" FontWeight="Bold" FontSize="20" FontFamily="Segoe UI Black"/>
        </StackPanel>
        <Label Content="" FontWeight="Bold" FontSize="20" FontFamily="Segoe UI Black" Name="nombreUsuario" Margin="82,89,304,235"/>
        <Button x:Name="btCerrar" Content="CERRAR SESIÓN" HorizontalAlignment="Left" Margin="448,336,0,0" VerticalAlignment="Top" Height="37" Width="221" Background="#FF000A5D" Foreground="White" FontSize="18" Click="btCerrar_Click"/>
    </Grid>
</Window>
